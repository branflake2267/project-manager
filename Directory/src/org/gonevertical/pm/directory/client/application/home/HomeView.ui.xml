<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .widget {
        }
        .inline {
            display: inline-table;
        }
    </ui:style>
    <g:HTMLPanel styleName="{style.widget}">
        <g:FlowPanel ui:field="displayPanel">
            <g:SimplePanel ui:field="archetypeList" styleName="{style.inline}" />
            <g:SimplePanel ui:field="archetypeDisplay" styleName="{style.inline}" />
        </g:FlowPanel>
        <g:SimplePanel ui:field="editPanel"/>
    </g:HTMLPanel>
</ui:UiBinder>
